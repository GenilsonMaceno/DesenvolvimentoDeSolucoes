<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!--To inherit the global NuGet package sources remove the <clear/> line below -->
    <clear />
    <add key="nuget" value="https://api.nuget.org/v3/index.json" />
	<add key="MyFeed" value="C:\mypackages\packageReferences" />
  </packageSources>
  <packageRestore>
	<add key="enabled" value="true" />
	<add key="automatic" value="true" />
  </packageRestore>

 <config>
  <clear />
    <!-- <add key="repositoryPath" value="C:\mypackages\packagesdeveloper" /> -->
    <add key="globalPackagesFolder" value="C:\mypackages\packageReferences" />
 </config>
</configuration>
